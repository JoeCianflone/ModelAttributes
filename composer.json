{
    "name": "joecianflone/model-attributes",
    "description": "A simple package to make the DX of working with Laravel Eloquent models better",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JoeCianflone\\ModelAttributes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joe Cianflone",
            "email": "JoeCianflone@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "crell/attributeutils": "^0.8.1",
        "illuminate/collections": "^9.31"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.7",
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "static": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
